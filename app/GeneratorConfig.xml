		<table tableName="${tableName11}" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" selectByExampleQueryId="false"/>
		<table tableName="${tableName12}" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" selectByExampleQueryId="false"/>
		<table tableName="${tableName13}" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" selectByExampleQueryId="false"/>
		<table tableName="${tableName14}" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" selectByExampleQueryId="false"/>
		<table tableName="${tableName15}" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" selectByExampleQueryId="false"/>	
	</context>
	<context id="oracle" targetRuntime="MyBatis3" defaultModelType="flat"> 
		<!-- 分隔符 -->
		<property name="beginningDelimiter" value="&quot;"/>
		<property name="endingDelimiter" value="&quot;"/>
		<property name="javaFileEncoding" value="UTF-8"/>
		<!-- 注释 -->
		<commentGenerator type="my.comment.generator.MyCommentGenerator">
			<property name="suppressDate" value="true" />
			<property name ="addRemarkComments" value="true"/>
		</commentGenerator>
		<!-- 数据源 -->
		<jdbcConnection driverClass="${jdbcDriver}" connectionURL="${connectURL}" userId="${userId}"  password="${password}">
			<property name="remarksReporting" value="true"/>
		</jdbcConnection>
		<!-- 实体类 -->
		<javaModelGenerator targetPackage="${javaModelPN}" targetProject="${javaModeDir}"> 
		    <property name="trimStrings" value="true" /> 
		</javaModelGenerator> 
		<!-- xml文件 -->
		<sqlMapGenerator targetPackage="${XmlFilePN}"  targetProject="${XmlFileDir}"/>
		<!-- 接口 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="${InterfacePN}"  targetProject="${InterfaceDir}"/> 
		<!-- 数据库表，可配置多个 -->
		<table schema="${schema}" tableName="${tableName0}" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false"  enableSelectByExample="false" selectByExampleQueryId="false"/>
